{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "airbnb", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "react/destructuring-assignment": "off",
        "react/prop-types": "off",
        "react/jsx-filename-extension": [0],
        "no-console": "off",
        "global-require": "off",
        "no-return-await": "off",
        "no-restricted-syntax": "off",
        "guard-for-in":  "off",
        "no-case-declarations": "off"
    }
}
